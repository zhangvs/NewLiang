            //$('.phone-ul').on('click', '.phone-li', function () {
            //    let mobile_id = $(this).attr('data-id');
            //    location.href = "/mobile/index/product.html?mobile_id=" + mobile_id;
            //});
            $('.precise-search-input').on('input propertychange', function () {
                let obj = $(this);
                if (obj.val() != '') {
                    obj.next('.precise-search-input').focus();
                } else {
                    obj.prev('.precise-search-input').focus();
                }
            });
            $('.to-service').click(function () {
                location.href = "express";
            });
            randnotice()
        });
        mui(".param-content").on('swiperight', '.param-div-sidebar', function () {
            toggleSidebar();
        });
        $('.mask').click(function () {
            toggleSidebar();
        });
        //开关侧边栏
        function toggleSidebar() {
            pageInfo['page'] = 1;
            let time = 300;
            let target = $('.param-div-sidebar');
            let distance = "20%";
            let isShow = 1;
            let maskDisplay = 'block';
            if (target.attr('data-show') == 1) {
                distance = "100%";
                isShow = 0;
                maskDisplay = "none";
            }
            $('.mask').css('display', maskDisplay);
            target.animate({ 'left': distance }, time, function () {
                target.attr('data-show', isShow);
            });
        }

        function autoCity(res) {
            if (!res.hasOwnProperty('status')) {
                return false;
            }
            if (!res.hasOwnProperty('result')) {
                return false;
            }
            let locinfo = res.result.ad_info;
            if (locinfo.province != '') {
                pageInfo['province'] = locinfo.province;
            }
            if (locinfo.city != '') {
                pageInfo['city'] = locinfo.city;
            }

            changeCity();
        }

        function renderProvince(arr) {
            let target = $('.param-province-ul');
            let tpl = '<li class="mui-table-view-cell param-province-li param-province-li-append" data-id ="__NAME__" data-pcode="__PID__">__NAME__</li>';
            $.each(arr.provices, function (i, v) {
                let lihtml = tpl.replace(/__PID__/, v.code);
                lihtml = lihtml.replace(/__NAME__/g, v.name);
                target.append(lihtml);
            });
        }

        function renderCity(arr) {
            let target = $('.param-city-ul');
            let tpl = '<li class="mui-table-view-cell  param-city-li param-city-li-render" data-id = "__NAME__">__NAME__</li>';
            let html = '<li class="mui-table-view-cell  param-city-li" data-id = "">全部</li>';
            $.each(arr, function (i, v) {
                let lihtml = tpl.replace(/__NAME__/g, v.name);
                html += lihtml;
            });
            target.empty();
            target.append(html);
        }

        function changeCity() {
            $('.param-loc').removeClass('param-span-active');
            let target = $('.cityname');
            if (pageInfo['city'] != '') {
                target.text(pageInfo['city']);
            } else if (pageInfo['province'] != '') {

                target.text(pageInfo['province']);
            } else {
                target.text('号码归属地');
            }
            getNum();
        }

        function changePageInfo(key, value) {
            pageInfo[key] = value;
            pageInfo['page'] = 1;
            getNum();
        }
        // function para*/
        function getNum(append = 0) {
            if (append == 0) {
                document.body.scrollTop = document.documentElement.scrollTop = 0
            }
            pageInfo['is_loading'] = 1;
            $('#loading').show();
            $('.param-div').hide();
            $.ajax({
                url: "ListData",
                type: 'POST',
                dataType: 'json',
                data: pageInfo,
                success: function (res) {
                    console.log(res);
                    //pageInfo['nomore'] = res.data.noMore;
                    if (res.type != 1) {
                        mui.toast('服务器错误!');
                        return false;
                    }
                    if (append == 0) {
                        $('.phone-ul').empty();
                    }

                    $('.phone-ul').append(res.resultdata + '<div class = "clear"></div>');
                    $('#loading').hide();
                    pageInfo['is_loading'] = 0;
                    //if (res.data.notEnough == 1) {
                    //    $('.pullLoad').text('-该地区库存不足,建议修改筛选条件-');
                    //}
                }
            });
        }



        function randnotice() {
            let prefix = ['165', '170', '167', '171', '162'];
            let end = ['1234', '2233', '2266', '8888', '6666', '2324', '6268', '7878', '9999', '9000', '8000', '8899', '8866', '7799', '5678', '6789', '1234', '9900'];
            let name = ['张', '王', '李', '赵', '袁', '朱', '宋', '金'];
            let indexPrefix = randomNum(0, prefix.length - 1);
            let indexEnd = randomNum(0, end.length - 1);
            let indexName = randomNum(0, name.length - 1);
            let step = randomNum(1, 10);
            let lastNum = getCookie('lastNum');
            if (lastNum == "NaN") {
                lastNum = 200;
            }
            if (lastNum == null) {
                newNum = 200;
            } else {
                newNum = parseInt(lastNum) + parseInt(step);
            }

            setCookie('lastNum', newNum);
            let newPhonenum = prefix[indexPrefix] + '****' + end[indexEnd];
            let noticeCount = '<span>已有</span><span>' + newNum + '</span><span>人订购</span>';
            let noticeMsg = '<span>' + name[indexName] + '</span><span>**获得</span>' + '<span>' + newPhonenum + '</span>';
            $('.notice-buy-num').html(noticeCount);
            $('.notice-content').html(noticeMsg);
        }

        function changeNotice() {
            randnotice();
            setInterval(function () {
                randnotice();
            }, 2000);
        }
        //生成从minNum到maxNum的随机数
        function randomNum(minNum, maxNum) {
            switch (arguments.length) {
                case 1:
                    return parseInt(Math.random() * minNum + 1, 10);
                    break;
                case 2:
                    return parseInt(Math.random() * (maxNum - minNum + 1) + minNum, 10);
                    //或者 Math.floor(Math.random()*( maxNum - minNum + 1 ) + minNum );
                    break;
                default:
                    return 0;
                    break;
            }
        }


        $(window).scroll(function () {

            let filterHeight = document.getElementById('phone-filter-box').offsetTop;
            //已经滚动到上面的页面高度
            let scrollTop = $(this).scrollTop();
            //页面高度
            let scrollHeight = $(document).height();
            //浏览器窗口高度
            let windowHeight = $(this).height();
            if ((windowHeight - scrollTop) < filterHeight) {
                $('#backToTop').show();
                $('#phone-filter-box').addClass('filter-top-show');
            } else {
                $('#backToTop').hide();
                $('#phone-filter-box').removeClass('filter-top-show');
            }

            //此处是滚动条到底部时候触发的事件，在这里写要加载的数据，或者是拉动滚动条的操作
            if (scrollTop + windowHeight >= scrollHeight - 20) {

                if (pageInfo['nomore'] == 1) {
                    return false;
                }
                if (pageInfo['is_loading'] == 1) {
                    return false;
                }
                pageInfo['page'] = pageInfo['page'] + 1;
                getNum(1);
            }
        });
        //dingda
    </script>

