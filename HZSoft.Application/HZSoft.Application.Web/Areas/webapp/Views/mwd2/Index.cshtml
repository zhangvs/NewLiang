                changePageInfo('Grade', $(this).attr('data-id'));
            });
            $('.filter-search-end').click(function () {
                let checked = $(this).is(':checked');
                if (!checked) {
                    changePageInfo('search_is_end', '0');
                } else {
                    changePageInfo('search_is_end', '1');
                }
            });
            //价格
            $('.price-li').click(function () {
                $('.price-li').removeClass('filter-li-active');
                $(this).addClass('filter-li-active');
                changePageInfo('price', $(this).attr('data-id'));
            });
            //省份
            $('.param-province-ul').on('click', '.param-province-li', function () {
                let pcode = $(this).attr('data-pcode');
                let pname = $(this).text();
                $('.param-province-li').removeClass('province-li-active');
                $(this).addClass('province-li-active');
                if (pcode == 0) {
                    pageInfo['province'] = '';
                    pageInfo['city'] = '';
                    pageInfo['page'] = 1;
                    changeCity();
                    return false;
                }

                let target = $('.param-city-ul');
                let tpl = '<li class="mui-table-view-cell  param-city-li param-city-li-render" data-id = "__NAME__">__NAME__</li>';
                let html = '<li class="mui-table-view-cell  param-city-li" data-id = "">全部</li>';

                if (pcode == "500000" || pcode == "110000" || pcode == "120000" || pcode == "310000") {
                    html += '<li class="mui-table-view-cell  param-city-li" data-id = "">' + pname + '</li>';
                    target.empty();
                    target.append(html);
                }
                else {
                    $.ajax({
                        url: 'https://datavmap-public.oss-cn-hangzhou.aliyuncs.com/areas/csv/' + pcode + '_city.json',
                        type: 'get',
                        success: function (res) {
                            $.each(res.rows, function (i, v) {
                                let lihtml = tpl.replace(/__NAME__/g, v.name);
                                html += lihtml;
                            });
                            target.empty();
                            target.append(html);
                        }
                    });
                }
            });
            //城市
            $('.param-city-ul').on('click', '.param-city-li', function () {
                let province = $('.province-li-active').attr('data-id');
                let city = $(this).attr('data-id');
                pageInfo['province'] = province;
                pageInfo['city'] = city;
                pageInfo['page'] = 1;
                changeCity();
                // getNum();
            });
            //paixu
            $('.param-orderby-div').on('click', 'span', function () {
                let datavalue = $(this).attr('data-value');
                $(this).addClass('param-orderby-span-active');
                $(this).siblings('span').removeClass('param-orderby-span-active');
                $('.param-span').removeClass('param-span-active');
                $('.param-orderby-div').hide();
                // pageInfo['ordername'] = datavalue;
                changePageInfo('ordername', datavalue);
            });
            //搜索框
            $('.filter-search-input').focus(function () {
                $('.filter1-search-btn').show();
            });

            $('.filter-search-input').blur(function () {
                if ($('.filter-search-input').val() == '') {
                    $('.filter1-search-btn').hide();
                    changePageInfo('search', '');
                }
            });
            $('.filter1-search-btn').click(function () {
                let searchval = $('.filter-search-input').val();
                let reg = /^[1-9]+[0-9]*]*$/;
                if (!reg.test(searchval)) {
                    mui.toast('请输入纯数字');
                    return false;
                }
                pageInfo['precise_num'] = '';
                changePageInfo('search', searchval == null ? 0 : searchval);
            });
            $('.precise-search-submit').click(function () {
                pageInfo['search'] = '';
                let preciseNum = '';
                $('.precise-data').each(function () {
                    let num = $(this).val() == '' ? '_' : $(this).val();
                    preciseNum += num;
                });
                changePageInfo('precise_num', preciseNum);
            });
            $('.filter-search-switch').click(function () {
                $('.filter-search-div').hide();
                $('.precise-search').show();
            });
            $('.precise-search-switch').click(function () {
                $('.precise-search').hide();
                $('.filter-search-div').show();
            });
            //筛选
