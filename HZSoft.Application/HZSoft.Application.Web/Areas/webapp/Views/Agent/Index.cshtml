            <section class="team_info">
                <h3 class="i_icon"><strong>概况</strong></h3>
                <div class="list flex comissionSummary">
                    <div class="team_item">
                        <h4 class="level_name i_icon"><label>1</label><strong>下级代理</strong></h4>
                        <strong style="margin: 20px;font-size: 20px;">@Model.childcount</strong><span>人</span>
                    </div>
                    <div class="team_item">
                        <h4 class="level_name i_icon"><label>2</label><strong>浏览量</strong></h4>
                        <strong style="margin: 20px;font-size: 20px;">@Model.SeeCount</strong><span>次</span>
                    </div>
                    <div class="team_item">
                        <h4 class="level_name i_icon"><label>3</label><strong>销售额</strong></h4>
                        <strong style="margin: 20px;font-size: 20px;">@Model.SellCount</strong><span>元</span>
                    </div>
                </div>
            </section>
            <section class="link_list">
                @if (Model.Category < 4)
    {
