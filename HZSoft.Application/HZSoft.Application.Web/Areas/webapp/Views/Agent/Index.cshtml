        function showhide() {
            let balance = '@Model.profit';
            let locked_balance = '@Model.Cashout';
            let tp = $('.balance').attr('data-type');
            if (tp == "password") {
                $('.balance').text(balance);
                $('.locked-balance').text(locked_balance);
                $('.balance').attr('data-type', 'number')
                eye.className = 'fa fa-eye-slash'
            } else {
