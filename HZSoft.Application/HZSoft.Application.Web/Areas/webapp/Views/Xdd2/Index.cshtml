                } //modifide

            }
            else {
                $("#kw").val(_kw.replace('|', ''));
                if (_kw.indexOf('|') > -1) { $("#cbIsEnd").attr('checked', true); }
                if (type == 'mb') {
                    $("#kwtit1").click();
                    so_kw_tab(1);
                } //modifide

            }
        }

        if (type == 'tel') {
            $(".alt-tabs .head").hide();

            $(".company .right").hide();
            $(".company .left").width("100%");
        }

        if ($(".filter-bar-city").html() != '归属地') { $(".filter-bar-city").addClass("red"); } else { $(".filter-bar-city").removeClass("red"); }
        if ($(".filter-bar-company").html() != '运营商') { $(".filter-bar-company").addClass("red"); } else { $(".filter-bar-company").removeClass("red"); }
        if ($(".filter-bar-featrue").html() != '号码类型') { $(".filter-bar-featrue").addClass("red"); } else { $(".filter-bar-featrue").removeClass("red"); }
        /*init*/

        /*action*/
        $("#searchNum input").keyup(function (event) {
            switch (event.which) {
                case 8: //back
                    $(this).prev().focus(); break;
                case 37: //left
                    $(this).prev().focus(); break;
                case 39: //right
                    $(this).next().focus(); break;
                default:
                    var t = $(this).val();
                    var reg = /[\d]/g;
                    if (reg.test(t)) {
                        $(this).next().focus();
                    }
                    else {
                        t = t.replace(/[^\d]/g, '');
                        $(this).val(t);
                    }
                    break;
            }
        });

        function searchBykw() {
            var _kw = '';
            if ($("#kwtit1").hasClass('cur') || $(".so-kw-type").html() == '模糊') {
                _kw = $("#kw").val();
                if (_kw != '') {
                    if ($("#cbIsEnd").is(':checked')) { _kw += "|"; }
                }
            } else {
                var o = document.getElementById('searchNum').getElementsByTagName('input');
                for (i = 0; i < o.length; i++) {
                    if (o[i].value == '') {
                        _kw += '_';
                    }
                    else {
                        _kw += o[i].value;
                    }
                }
            }
            if (_kw != '' && _kw != '1__________') {
                //do search
                keyword = _kw;
                so2();
            }
            else {
                //未输入关键字
                keyword = _kw;
                so2();
            }
        }

        $(".kw-section .btn-submit").click(function () {
            searchBykw();
        });

        $("#so-kw-btn").click(function () {
            searchBykw();
        })


        $(".filter-bar > div").click(function () {
            if ($(this).hasClass("nav-sel")) {
                hideAllFilterItem();
            }
            else {
                $(this).siblings().removeClass("nav-sel");
                $(this).addClass("nav-sel");
                $(".mask").show();
                $(".filter-pop").show();
                $(".filter-pop > div").hide().eq($(this).index()).show();
            }
        })

        function hideAllFilterItem() {
            $(".mask").hide();
            $(".filter-pop").hide();
            $(".filter-bar > div").removeClass("nav-sel");
        }

        $(".mask").click(function () {
            hideAllFilterItem();
        })

        //归属地
        $(".city .left li").click(function () {
            $(this).siblings().removeClass("cur");
            $(this).addClass("cur");
            $(".city .right li[data-provinceid='" + $(this).data("provinceid") + "']").show();
            $(".city .right li[data-provinceid!='" + $(this).data("provinceid") + "']").hide();
            $(".city .right li[data-cityid='0']").show();
        })
        $(".city .right li").click(function () {
            city = $(this).data("cityid");
            if (city == '0') {
                city = $(".city .left .cur").data("provinceid");
                var pname = $(".city .left .cur").text();
                if (pname == '' || pname == '全部') { pname = '归属地'; }
                $(".filter-bar-city").html(substr(pname, 0, 3, true)).removeClass("nav-sel");
            } else {
                $(".filter-bar-city").html(substr($(this).data("cityname"), 0, 3, true)).removeClass("nav-sel");
            }
            if ($(".filter-bar-city").html() != '归属地') { $(".filter-bar-city").addClass("red"); } else { $(".filter-bar-city").removeClass("red"); }
            hideAllFilterItem();
            //do search
            reload();
        })

        //运营商
        $(".company .left li").click(function () {
            $(this).siblings().removeClass("cur");
            $(this).addClass("cur");
            if (type == "tel") {
                company = $(this).data("companyid");
                hideAllFilterItem();
                reload();
            } else {
                if ($(this).data("companyid") == "0") {
                    $(".company .right li").show();
                } else {
                    $(".company .right li[data-companyid='" + $(this).data("companyid") + "']").show();
                    $(".company .right li[data-companyid!='" + $(this).data("companyid") + "']").hide();
                    $(".company .right li[data-companyid='0']").show();
                }
            }
        })
        $(".company .right li").click(function () {
            frontNumber = $(this).data("prefixnum");
            if (frontNumber == '') {
                company = $(".company .left .cur").data("companyid");
                var pname = $(".company .left .cur").data("companyname");
                if (pname == '' || pname == '全部') { pname = '运营商'; }
                $(".filter-bar-company").html(substr(pname, 0, 4, true)).removeClass("nav-sel");
            } else {
                var tit = $(this).data("prefixnum").toString();
                tit = substr(tit, 0, 4, true);
                $(".filter-bar-company").html(tit).removeClass("nav-sel");
            }
            if ($(".filter-bar-company").html() != '运营商') { $(".filter-bar-company").addClass("red"); } else { $(".filter-bar-company").removeClass("red"); }
            hideAllFilterItem();
            //do search
            reload();
        })

        //号码类型
        $(".featrue .left li").click(function () {
            $(this).siblings().removeClass("cur");
            $(this).addClass("cur");
            pos = $(this).data("pos");
        })
        $(".featrue .right li").click(function () {
            $(this).siblings().removeClass("cur");
            $(this).addClass("cur");
            features = $(this).data("fval");
            if (features == '') {
                $(".filter-bar-featrue").html('号码类型').removeClass("nav-sel");
            } else {
                $(".filter-bar-featrue").html(substr($(this).text(), 0, 5, true)).removeClass("nav-sel");
            }
            if ($(".filter-bar-featrue").html() != '号码类型') { $(".filter-bar-featrue").addClass("red"); } else { $(".filter-bar-featrue").removeClass("red"); }
            hideAllFilterItem();
            //do search
            reload();
        })

        //排序
        $(".orderItem").click(function () {
            $(this).siblings().removeClass("cur");
            $(this).addClass("cur");
            orderType = $(this).data("val") || '';
            //do search
            reload();
        })

        //更多筛选
        $(".filter-more li:not(.ckb-item)").click(function () {
            $(this).siblings().removeClass("cur");
            $(this).addClass("cur");
        })

        //价格
        $(".price li").click(function () {
            $(this).siblings().removeClass("cur");
            $(this).addClass("cur");
            var v = $(this).data("val");
            if (v == '') {
                $("#lowprice").val('');
                $("#topprice").val('');
            }
            else {
                $("#lowprice").val(v.split("-")[0]);
                $("#topprice").val(v.split("-")[1]);
            }
        })
        $("#lowprice").bind('input propertychange', function () {
            $(".price li").removeClass("cur");
        });
        $("#topprice").bind('input propertychange', function () {
            $(".price li").removeClass("cur");
        });

        //更多筛选>隐藏多选项之不限
        $(".filter-more li.ckb-item").each(function () {
            if ($(this).data("val") == "") {
                $(this).hide();
            }
        })
        //更多筛选>多选项点击效果
        $(".filter-more li.ckb-item").click(function () {
            if ($(this).hasClass("cur")) {
                $(this).removeClass("cur");
            } else {
                $(this).addClass("cur");
            }
        })

        $(".filter-more-btn .submit").click(function () {
            price = get_price();//$(".filter-more .price .cur").data("val") || '';
            except = get_except(); //$(".filter-more .except .cur").data("val") || '';
            //orderType=$(".filter-more .order .cur").data("val") || '';
            cat = $(".filter-more .cat .cur").data("val") || '';
            yuyi = $(".filter-more .yuyi .cur").data("val") || '';
            moreNumber = $(".filter-more .moreNumber .cur").data("val") || '';
            hideAllFilterItem();
            //do search
            reload();
        })

        function get_except() {
            var t = "";
            $(".filter-more .except .cur").each(function () {
                t += $(this).data("val") + "|";
            });
            return t;
        }

        function get_price() {
            var p1 = $("#lowprice").val();
            var p2 = $("#topprice").val();
            var t1 = parseInt(p1, 10);
            var t2 = parseInt(p2, 10);
            if (isNaN(t1)) t1 = 0;
            if (isNaN(t2)) t2 = 0;
            return t1 + "-" + t2;
        }

        $(".filter-more-btn .reset").click(function () {
            $(".filter-more li").removeClass("cur");
            $("#lowprice").val(""); $("#topprice").val("");
            price = '';
            except = '';
            orderType = '';
            cat = '';
            yuyi = '';
            moreNumber = '';
            hideAllFilterItem();
            //do search
            reload();
        })
        /*action*/

        /*function*/

        function so2() {
            var url = 'index?city=' + city + '&keyword=' + keyword + '&page=' + curpage + '&features=' + features + '&orderType=' + orderType + '&price=' + price + '&except=' + except + '&yuyi=' + yuyi;
            location.href = url;
        }

        function so() {
            $(".loading").css("display", "flex");
            var url = 'ListData?&city=' + city + '&keyword=' + keyword + '&page=' + curpage + '&features=' + features + '&orderType=' + orderType + '&price=' + price + '&except=' + except + '&yuyi=' + yuyi;
            $.get(url, function (result) {
                if (result == '') {
                    if (curpage == 1) {
                        $(".noresult").show();
                    } else {
                        $(".loadmore").html('没有了');
                    }
                }
                else {
                    result = SetMobileStyle_(1, result);
                    $("#data").append(result);
                    if (curpage == 1) {
                        $("#hasresult").show();
                        if ($("#hasresult ul li").length % 40 == 0) { $(".loadmore").show(); }
                    }
                }
                $(".loading").css("display", "none");
            });
        }

        $(".loadmore").click(function () {
            curpage = curpage + 1;
            so();
        });


        $(".loadmore").click(function () {
            curpage = curpage + 1;
            so();
        });

        function reload() {
            $("#hasresult").hide();
            $(".noresult").hide();
            $("#data li").remove();
            $(".loadmore").show();//2020.4.16 hide 临时改为 show,兼容下拉
            curpage = 1;
            so();
        }

        reload();

        function substr(str, start, stop, dot) {
            var re = str.substring(start, stop);
            if (dot) {
                if (str.length - 1 > stop - 1) {
                    re = re.substring(0, re.length - 1) + '..';//减少一个字再加...
                }
            }
            return re;
        }
        /*function*/

        $(window).scroll(function () {
            var scrollTop = $(this).scrollTop();
            var scrollHeight = $(document).height();
            var windowHeight = $(this).height();
            if (scrollTop + windowHeight == scrollHeight && scrollTop > 0) {
                $(".loadmore").click();
            }
            if (scrollTop > 200) { $(".gotop").show(); } else { $(".gotop").hide(); }
        });

        $(".gotop").click(function () {
            document.documentElement.scrollTop = document.body.scrollTop = 0;
        })



        function showKwSection() { $(".kw-section").show(); $(".kw-section .alt-tabs-head:first-child").click(); $(".kw-section .kw1 input[type=number]").focus(); }
        function hideKwSection() { $(".kw-section").hide(); }

        //SetMobileStyle(1);

        function so_kw_tab(t) {
            if (t == 2) {
                $(".so-kw-type").html("精确");
                $(".so-kw-input").hide();
                $("#searchNum").show();
            }
            else {
                $(".so-kw-type").html("模糊");
                $("#searchNum").hide();
                $(".so-kw-input").show();
            }
        }

        $(".so-kw-type").click(function () {
            var t = $(this).html();
            if (t == "模糊") {
                so_kw_tab(2);
            } else {
                so_kw_tab(1);
            }
        });

