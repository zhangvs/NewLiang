@using HZSoft.Application.Entity.CustomerManage;
<!DOCTYPE html>
<html>
<head>
    <title>苏宁互联我的互联</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=0" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="format-detection" content="telephone=no">
    <link href="~/Content/scripts/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/scripts/jquery/jquery-1.11.2.min.js"></script>
    <link href="~/Content/layer2/mobile/need/layer.css" rel="stylesheet" />
    <script src="~/Content/layer2/layer.js" type="text/javascript"></script>
    <script>
        var css = { fontSize: '2em' };
        setInterval(function () {
            $('.animate').animate(css, 3000, rowBack);
        }, 3000);
        function rowBack() {
            if (css.fontSize === '1em')
                css.fontSize = '2em';
            else if (css.fontSize === '2em')
                css.fontSize = '1em';
        }
    </script>    
    <!--分享-->
    <!--end分享-->
</head>
<body>
    <div>
        <div>
            <div>
                <div>
                    <div>
                        <div style="width: 100%;margin-left: auto;margin-right: auto;transform: rotateZ(0deg);">
                            <fieldset style="border-width: 0px;border-style: none;border-color: initial;font-family: 微软雅黑;">
                                <div style="border-width: 3px;border-style: solid;border-color: rgb(95, 170, 255);padding: 5px;">
                                    <div style="border-width: 1px;border-style: solid;border-color: rgb(95, 170, 255);padding: 15px;text-align: center;color: inherit;">
                                        <div style="font-size: 16px;white-space: normal;color: rgb(62, 62, 62);line-height: 25.6px;border-width: 0px;border-style: none;border-color: currentcolor;">
                                            <div style="padding: 10px;">
                                                <div style="border-width: 0px;border-style: none;border-color: initial;">
                                                    <div style="margin: 10px auto;">
                                                        <div style="margin-right: auto;margin-left: auto;width: 20px;height: 20px;border-right: 1px solid rgb(172, 29, 16);border-bottom: 1px solid rgb(172, 29, 16);transform: rotate(45deg);"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <h2 style="font-weight: bold;white-space: normal;color: rgb(62, 62, 62);line-height: 1.6em;"><span style="font-size: 14px;">苏宁互联 &nbsp;我的互联</span></h2>
                                        <p style="font-size: 16px;white-space: normal;color: rgb(62, 62, 62);line-height: 1.75em;background-color: rgb(254, 255, 255);"><span style="font-size: 14px;">尊贵 &nbsp;专享 &nbsp;品味</span></p>
                                        <p style="font-size: 16px;white-space: normal;"><span style="color: rgb(54, 96, 146);font-size: 18px;"><strong>月租6元，打全国1毛2，卡内含10-20元话费</strong></span></p>
                                        <p style="font-size: 16px;white-space: normal;"><span style="font-size: 24px;color: rgb(54, 96, 146);"><strong><strong style="font-size: 16px;line-height: 20.48px;text-align: justify;">公司地址：临沂市奥斯卡上品公馆2204号</strong></strong></span></p>
                                    </div>
                                </div>
                            </fieldset>
                        </div>
                        <p><br /></p>
                        <p><br /></p>
                        <div style="width: 100%;margin-left: auto;margin-right: auto;">
                            <div style="margin: 5px auto;">
                                <div style="background-image: url(/Content/images/kuang.jpg);background-size: contain;background-position: 50% 0px;background-repeat: repeat;border-radius: 1em;word-wrap: break-word !important;box-sizing: border-box !important;">
                                    <div style="box-sizing:border-box;padding:10px;color:inherit;">
                                        <div style="padding: 15px 10px 20px;box-sizing: border-box;background-color: rgb(146, 195, 255);border-radius: 1em;color: rgb(255, 255, 255);border-color: rgb(146, 195, 255);text-align: center;">
                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="border-color: rgb(146, 195, 255);"></span><span style="color: rgb(255, 0, 0);letter-spacing: 0em;">★★★★★&nbsp;</span></p>
                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="color: rgb(255, 0, 0);letter-spacing: 0em;">精品谱号</span></p>
                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="color: rgb(255, 0, 0);letter-spacing: 0em;">一家专门做谱号的供应商</span></p>

                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><strong><span style="font-size: 18px;color: rgb(0, 0, 0);">5A</span></strong><img src="~/Content/images/0xing.gif" /></p>
                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="color: rgb(255, 255, 255);letter-spacing: 0em;">号码/归属地/价格(元)</span></p>
                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="color: rgb(255, 255, 255);letter-spacing: 0em;"></span></p>
                                            @{
                                                List<TelphoneSourceEntity> list = ViewBag.list;

                                                foreach (var bl in list)
                                                {
                                                    <p>@bl.Telphone &nbsp;临沂移动 &nbsp;原价39.8限时免费 </p>
                                                }
                                            }

                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="letter-spacing: 0em;color: rgb(255, 0, 0);">★★★★★</span></p>
                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="font-size: 18px;letter-spacing: 0em;color: rgb(255, 0, 0);">精品谱号</span></p>
                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="font-size: 18px;letter-spacing: 0em;color: rgb(255, 0, 0);">精于专业 力求卓越</span></p>
                                            <p style="box-sizing: border-box;color: rgb(146, 195, 255);font-size: 14px;line-height: 30px;border-color: rgb(146, 195, 255);"><span style="font-size: 18px;letter-spacing: 0em;color: rgb(255, 0, 0);">▼</span></p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="editor">
                            <p style="text-align:center;"><img src="~/Content/images/0tan.gif" /></p>
                            <p style="text-align:center;"><br /></p>
                        </div>
                        <div style="height:50px" class="animate">总有一个号码适合您</div>
                        <img src="~/Content/images/0.gif" class="img-responsive center-block">
                        <p style="max-width: 100%;min-height: 1em;color: rgb(62, 62, 62);text-align: center;"><span style="font-size: 14px;font-weight: bold;background-color: rgb(255, 255, 255);font-family: 微软雅黑;color: rgb(103, 2, 248);">各位亲们！赶快来抢购吧，来晚了可能就买不到啦！~</span></p>
                    </div>
                </div>
            </div>
        </div>



    </div>
</body>
</html>
