@model HZSoft.Application.Entity.WeChatManage.WeChat_UsersEntity
@{
    ViewBag.Title = "个人中心";
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name='apple-touch-fullscreen' content='yes' />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="format-detection" content="telephone=no" />
    <meta name="format-detection" content="address=no" />
    <meta name="viewport" content="initial-scale=1, width=device-width, maximum-scale=1, user-scalable=no" />
    <link href="/Content/Styles/stylelist.css" rel="stylesheet" type="text/css" />
    <script src="~/Content/scripts/jquery/jquery-2.0.3.min.js" type="text/javascript"></script>
    <title>个人中心</title>
    <style>
        .menu_grzx {
            top: 190px;
        }
    </style>
    <script>
        $(function () {
        })
    </script>
</head>
<body>
    <div style="background: #06C1AE; background-repeat: no-repeat; background-size: cover;
        /* height: 4rem; */
    vertical-align: middle;">
        <header class="bg01" style="padding: initial; position: initial;">
            <span style="margin-right: 5%; font-size: 0.32rem;" class="fr">
                <a href="Edit" style="color: #fff;">
                    编辑资料
                </a>
            </span>
        </header>
        <div style="padding-left: .15rem;padding-bottom: .15rem;">
            <table border="0" cellspacing="0" cellpadding="0" style="padding-top: 2rem; vertical-align: middle;">
                <tbody>
                    <tr>
                        <td rowspan="3" style="color: #fff;">
                            <div class="upic" style="background: url(@Model.HeadimgUrl) no-repeat center center; background-size: cover;">
                            </div>
                        </td>
                        <td style="width: 0.3rem; color: #fff;" rowspan="3"></td>
                        <td style="font-size: 0.35rem; color: #fff;">
                            @Model.NickName
                        </td>
                    </tr>
                    <tr>
                        <td class="czjb" style="color: #fff; padding-top: 10px;">
                            级别：
                            <img src="/Content/images/icon_czjb1.png" id="levelimg" style="height: 20px;" />
                        </td>
                    </tr>
                    <tr>
                        <td id="intro" style="color: #fff;">
                            推荐人：石崇靓号
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        @*<div class="menu_grzx" style="margin-top: .15rem; -webkit-margin-before:.15rem; position: initial; top: initial;">
        </div>*@
    </div>
    <div class="adress" style="padding: 1.3% 4%; background: #fff; line-height: 150%;">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr onclick="window.location='/WeiXinModule/userCenter/MyWallet'">
                <td width="50%" align="center" style="font-size: 0.23rem; color: #9a9a9a;">
                    靓号总数(个)
                </td>
                <td width="50%" align="center" style="font-size: 0.23rem; color: #9a9a9a;">
                    已赢利(元)
                </td>
            </tr>
            <tr>
                <td align="center" style="font-size: 0.32rem;" id="usablemoney">
                    123
                </td>
                <td align="center" style="font-size: 0.32rem;" id="cashedmoney">
                    456
                </td>
            </tr>
        </table>
    </div>
    <div class="wddd" style="margin-right: 0; padding-right: 0;">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr onclick="javascript:location.href='MyShare'">
                <td style="color: #e74c2e;">
                    <img src="/Content/Images/二维码@2x.png" style="height: 25px; margin-right: 5px; padding-bottom: 5px;" />
                    <a href="MyShare" style="color: Red;">点击生成分享赚钱二维码</a>
                </td>
                <td width="34%" align="right">
                    <a href="MyShare">&nbsp; &nbsp;<img src="~/Content/images/btn_arrow1.png" width="24%" /></a>
                </td>
            </tr>
        </table>
    </div>
    <div class="wddd" style="margin-right: 0; padding-right: 0;">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr onclick="javascript:location.href='BonusPlan'">
                <td>
                    <img src="/Content/Images/奖金@2x.png" style="height: 25px; margin-right: 5px; padding-bottom: 5px;" /><a href="BonusPlan">洗车补贴方案</a>
                </td>
                <td width="36%" align="right">
                    <a href="BonusPlan">
                        <img src="~/Content/images/btn_arrow1.png" width="24%" />
                    </a>
                </td>
            </tr>
            <tr onclick="javascript:location.href='MyOrderList'">
                <td>
                    <img src="/Content/Images/订单@2x.png" style="height: 25px; margin-right: 3px; margin-bottom: 5px;" />
                    <a href="MyOrderList">我的订单</a>
                </td>
                <td width="34%" align="right">
                    <a href="MyOrderList">
                        <img src="~/Content/images/btn_arrow1.png" width="24%" />
                    </a>
                </td>
            </tr>
            <tr onclick="javascript:location.href='MyCarList'">
                <td>
                    <img src="/Content/Images/车辆@2x.png" style="height: 25px; margin-right: 5px; padding-bottom: 5px;" /><a href="MyCarList">我的车辆</a>
                </td>
                <td width="34%" align="right">
                    <a href="MyCarList">
                        <img src="~/Content/images/btn_arrow1.png" width="24%" />
                    </a>
                </td>
            </tr>
        </table>
    </div>
    <div class="wddd" style="margin-right: 0; padding-right: 0;">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr onclick="javascript:location.href='MyWallet'">
                <td>
                    <img src="/Content/Images/零钱@2x.png" style="height: 25px; margin-right: 5px; padding-bottom: 5px;" /><a href="MyWallet">我的钱包</a>
                </td>
                <td width="36%" align="right">
                    <a href="MyWallet">
                        <img src="~/Content/images/btn_arrow1.png" width="24%" />
                    </a>
                </td>
            </tr>
        </table>
    </div>
    <div class="wddd" style="margin-bottom: 1.3rem; margin-right: 0; padding-right: 0;">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
            <tr onclick="javascript:location.href='MyExtension'">
                <td>
                    <img src="/Content/Images/推广@2x.png" style="height: 25px; margin-right: 5px; padding-bottom: 5px;" /><a href="MyExtension">我的分销</a>
                </td>
                <td width="36%" align="right">
                    <a href="MyExtension">
                        <img src="~/Content/images/btn_arrow1.png" width="24%" />
                    </a>
                </td>
            </tr>
            <tr onclick="javascript:location.href='KeFu'">
                <td>
                    <img src="/Content/Images/在线客服@2x.png" style="height: 20px; margin-right: 5px; padding-bottom: 5px;" /><a href="KeFu">在线客服</a>
                </td>
                <td width="36%" align="right">
                    <a href="BonusPlan">
                        <img src="~/Content/images/btn_arrow1.png" width="24%" />
                    </a>
                </td>
            </tr>
            <tr onclick="javascript:location.href='versionInfo'">
                <td>
                    <img src="/Content/Images/版本信息@2x.png" style="height: 20px; margin-right: 5px; padding-bottom: 5px;" /><a href="KeFu">版本信息</a>
                </td>
                <td width="36%" align="right">
                    <a href="BonusPlan">
                        <img src="~/Content/images/btn_arrow1.png" width="24%" />
                    </a>
                </td>
            </tr>
        </table>
    </div>
    <div style="height: 20px;">
    </div>
    <nav class="ss1">
        <a href="/WeChatManage/Liang/Index" class="nav_home nav_home">首页</a> 
        <a href="/WeChatManage/UserCenter/Index" class="nav_grzx grzx_curr">个人中心</a>
    </nav>
</body>
</html>
