                width: 100%;
                height: 150px;
            }

        .dottedNav {
            width: 100%;
            height: 18px;
            line-height: 18px;
            text-align: center;
            position: absolute;
            z-index: 110;
            left: 0;
            bottom: 15px;
        }
    </style>
    <script type="text/javascript">
        var url = document.URL;
        if (url.indexOf('from=') > -1) {
            var name = 'fromurl';
            var value = url;
            var exp = new Date();
            exp.setTime(exp.getTime() + 3 * 24 * 60 * 60 * 1000);
            document.cookie = name + "=" + escape(value) + ";expires=" + exp.toGMTString() + ";path=/";
        }
    </script>
