@using HZSoft.Application.Entity.CustomerManage;
<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, viewport-fit=cover">
    <meta name="format-detection" content="telephone=no">
    <title>号运来靓号网</title>
    <link rel="stylesheet" href="/themes/simplebootx_mobile/Public/css/weui.min.css">
    <link rel="stylesheet" href="/themes/simplebootx_mobile/Public/css/jquery-weui.min.css">
    <link rel="stylesheet" href="/themes/simplebootx_mobile/Public/css/css.css">
    <meta name="__hash__" content="ba39410f768a25e6a11b9196fb6407ca_fe05e9c7ec61652835d66c78bb306176" />
    <style>
        .money-market {
            font-family: Arial, Helvetica, sans-serif;
            color: #E40011;
            text-decoration: line-through;
            color: #666;
            font-size: 12px;
            font-weight: normal;
        }
    </style>
</head>
<body class="index">
    <div class="top">
        <div class="nav">
            <a href="#" id="show-notice">活动攻略</a><a href="/H5/homepage/express">订单查询</a>
        </div>
        <img src="/themes/simplebootx_mobile/Public/images/banner.png">
    </div>
    <div class="select">
        <div class="select-title">
            <span>号码特征</span>
        </div>
        <div class="select-box">
            <a class="active" href="./?keyword=&city=&features=&orderType=">全部</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=0">超级精品号</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=1">AAAAAA</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=2">AAAAA</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=3">AAAA</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=11">3A(0-5)</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=12">3A(6-9)</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=4">双豹子(0-5)</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=5">ABCDE</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=6">ABCD</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=7">ABC</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=9">ABCDEF</a>
            | <a class="" href="./?keyword=&city=&orderType=&referer=&price=&features=13">大循环</a>
        </div>
        <div class="select-title">
            <span>号码价格</span>
        </div>
        <div class="select-box">
            <a class="active" href="./?keyword=&features=&city=&orderType=">全部</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=-300">300以下</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=301-500">301-500</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=501-1000">501-1000</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=1001-2000">1001-2000</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=2001-5000">2001-5000</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=5001-10000">5001-10000</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=10001-20000">1-2万</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=20001-30000">2-3万</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=30001-50000">3-5万</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=50001-70000">5-7万</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=70001-100000">7-10万</a>
            | <a class="" href="./?keyword=&features=&city=&orderType=&price=100000-">10万以上</a>
        </div>
    </div>
    <div class="search">
        <input type="text" id="keywords" class="input-mod" value="" placeholder="请输入您喜欢的号码" autocomplete="off">
        <a href="javascript:;" class="btn-search" data-url="/h5/homepage/index?keyword=keysval" id="search-submit">
            <i><img src="/themes/simplebootx_mobile/Public/images/sreach-icon.png"></i>搜索
        </a>
    </div>
    <div class="haoma">
        <div class="haoma-title">
            <a href="#" id='city-picker' data-url="/h5/homepage/index?city=cityvalue">
                归属地
                <i></i>
            </a><a href="#" id="manager-picker" data-url="/h5/homepage/index?manager=managervalue">
                选择网络
                <i></i>
            </a><a href="#" id="pricescope-picker" data-url="/h5/homepage/index?price=pricescopevalue">
                选择价格
                <i></i>
            </a>
        </div>
        <div class="haoma-list">
            <ul>
                @{
                        
                        List<TelphoneLiangEntity> list = ViewBag.list;
                        if (list != null)
                        {
                            if (list.Count > 0)
                            {
                                foreach (var bl in list)
                                {
                                    decimal? yj = bl.MaxPrice * 2;
                                    <li>
                                        <a href="/H5/homepage/mobileinfo/@bl.TelphoneID">
                                            <h1>
                                                @bl.Telphone
                                            </h1>
                                            <div>
                                                <span class="text-hidden">@bl.City@bl.Operator.Replace("\u7f51\u7edc","")</span>
                                                <p>
                                                    <font class="money-market">¥@yj</font>
                                                    <font class="color-red">¥@bl.MaxPrice</font>
                                                </p>
                                            </div>
                                        </a>
                                    </li>
                                }
                            }
                        }
                }
            </ul>
        </div>
    </div>
    <div class="tanchu-bg" style="display: none;">
    </div>
    <div class="tanchu2" style="display: none;">
        <div class="tanchu">
            <div class="tanchu-title">
                温馨提示
            </div>
            <div class="tanchu-box">
                <div class="wxts">
                    NO1.所有订单成功后，次日下午7点后发货，会有短信和语音信息通知；
                    <br />
                    NO2.查询快递单号，请登陆快递公司官网查询
                    <br />
                    NO3.快递均为到付件
                    <br />
                    NO4.
                    <span class="color-red">快递收到后》实名认证》充值缴费》正常使用</span><br />
                    NO5.客服热线0377-63078038
                </div>
            </div>
        </div>
        <div class="tanchu-close">
            <a href="#"><img src="/themes/simplebootx_mobile/Public/images/close.png"></a>
        </div>
    </div>
</body>
</html>
<script src="/themes/simplebootx_mobile/Public/js/jquery.min.js"></script>
<script src="/themes/simplebootx_mobile/Public/js/jquery-weui.min.js"></script>
<script src="/themes/simplebootx_mobile/Public/js/city-picker.min.js"></script>
