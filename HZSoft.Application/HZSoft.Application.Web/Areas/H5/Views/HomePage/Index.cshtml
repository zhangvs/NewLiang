        pricescope_display.push('选择价格');
        pricescope['选择价格'] = -1;
        $.each(config.pricescope, function (key, val) {
            pricescope_display.push(val);
            pricescope[val] = key;
        });
        manager_display.push('选择网络');
        manager['选择网络'] = 0;
        $.each(config.manager, function (key, val) {
            if (val.id < 4) {
                var name = val.name;
                manager_display.push(name);
                manager[name] = val.id;
            }
        });
        $("#manager-picker").picker({
            title: "网络",
            cols: [{
                textAlign: 'center',
                values: manager_display
            }],
            onClose: function (e) {
                var key = e.displayValue[0];//移动网络
                //var val = manager[key];//1
                var url = $("#manager-picker").data('url');
                url = url.replace('managervalue', key);
                window.location.href = url;
            }
        });
        $("#pricescope-picker").picker({
            title: "价格",
            cols: [{
                textAlign: 'center',
                values: pricescope_display
            }],
            onClose: function (e) {
                var key = e.displayValue[0];//0-100
                //var val = pricescope[key];//1
                var url = $("#pricescope-picker").data('url');
                url = url.replace('pricescopevalue', key);
                window.location.href = url;
            }
        });
        $("#city-picker").cityPicker({
            title: "请选择归属地",
            showDistrict: false,
            onClose: function (e) {
                var province = e.displayValue[0];
                var city = e.displayValue[1];
                if (city != '选择市') {
                    var url = $("#city-picker").data('url');
                    url = url.replace('cityvalue', city);
                    url = url.replace('provincevalue', province);
                    window.location.href = url;
                }
            }
        });
        $('#search-submit').bind('click', function () {
            var url = $(this).data('url');
            var keys = $('#keywords').val();
            url = url.replace('keysval', keys);
            window.location.href = url;
        });
        $('#show-notice').bind('click', function () {
            $('.tanchu-bg').show();
            $('.tanchu2').show();
        });
        $('.tanchu-close').bind('click', function () {
            $('.tanchu-bg').hide();
            $('.tanchu2').hide();
        });
    })
</script>